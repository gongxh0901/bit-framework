{
    "name": "@gongxh/bit-condition",
    "version": "0.0.6",
    "description": "基于creator的条件系统库",
    "main": "./dist/bit-condition.cjs",
    "module": "./dist/bit-condition.mjs",
    "types": "./dist/bit-condition.d.ts",
    "exports": {
        ".": {
            "require": "./dist/bit-condition.cjs",
            "import": "./dist/bit-condition.mjs",
            "types": "./dist/bit-condition.d.ts",
            "default": "./dist/bit-condition.cjs"
        },
        "./min": {
            "require": "./dist/bit-condition.min.cjs",
            "import": "./dist/bit-condition.min.mjs"
        }
    },
    "scripts": {
        "clean": "rm -rf dist",
        "build": "pnpm run clean && rollup -c rollup.config.mjs"
    },
    "files": [
        "dist"
    ],
    "author": "bit老宫",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/gongxh0901/bit-framework.git",
        "directory": "bit-condition"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "dependencies": {
        "@gongxh/bit-core": "workspace:*",
        "fairygui-cc": "^1.2.2"
    }
}
