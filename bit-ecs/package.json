{
    "name": "@gongxh/bit-ecs",
    "version": "0.0.6",
    "description": "一个ts实现的ECS系统",
    "main": "./dist/bit-ecs.cjs",
    "module": "./dist/bit-ecs.mjs",
    "types": "./dist/bit-ecs.d.ts",
    "exports": {
        ".": {
            "require": "./dist/bit-ecs.cjs",
            "import": "./dist/bit-ecs.mjs",
            "types": "./dist/bit-ecs.d.ts",
            "default": "./dist/bit-ecs.cjs"
        },
        "./min": {
            "require": "./dist/bit-ecs.min.cjs",
            "import": "./dist/bit-ecs.min.mjs"
        }
    },
    "scripts": {
      "dev": "ts-node-dev --respawn src/index.ts",
      "clean": "rm -rf dist",
      "build": "pnpm run clean && rollup -c rollup.config.mjs"
  },
  "files": [
      "dist/bit-ecs.cjs",
      "dist/bit-ecs.mjs",
      "dist/bit-ecs.min.cjs",
      "dist/bit-ecs.min.mjs",
      "dist/bit-ecs.d.ts"
  ],

    "author": "bit老宫",
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "https://github.com/gongxh0901/bit-framework.git",
      "directory": "bit-ecs"
    },
    "publishConfig": {
      "registry": "https://registry.npmjs.org/",
      "access": "public"
    }
} 