{
    "name": "@gongxh/bit-net",
    "version": "0.0.6",
    "description": "网络相关",
    "main": "./dist/bit-net.cjs",
    "module": "./dist/bit-net.mjs",
    "types": "./dist/bit-net.d.ts",
    "exports": {
        ".": {
            "require": "./dist/bit-net.cjs",
            "import": "./dist/bit-net.mjs",
            "types": "./dist/bit-net.d.ts",
            "default": "./dist/bit-net.cjs"
        },
        "./min": {
            "require": "./dist/bit-net.min.cjs",
            "import": "./dist/bit-net.min.mjs"
        }
    },
    "scripts": {
        "clean": "rm -rf dist",
        "build": "pnpm run clean && rollup -c rollup.config.mjs"
    },
    "files": [
        "dist/bit-net.cjs",
        "dist/bit-net.mjs",
        "dist/bit-net.min.cjs",
        "dist/bit-net.min.mjs",
        "dist/bit-net.d.ts",
        "libs/lib.ali.api.d.ts",
        "libs/lib.bytedance.api.d.ts",
        "libs/lib.wx.api.d.ts"
    ],
    "author": "bit老宫",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/gongxh0901/bit-framework.git",
        "directory": "bit-net"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    }
}
