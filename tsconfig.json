{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "experimentalDecorators": true,
    "strict": true,
    "strictNullChecks": false,
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "stripInternal": true,
    "types": [
      "@cocos/creator-types/engine",
      "@cocos/creator-types/editor"
    ]
  },
  "exclude": [
    "node_modules",
    "dist",
    "build"
  ]
}
